export plugname=rdc_sampler
portbase=61060
dschema=$(ldms_rdc_schema_name shape=1|grep -v ERR)
wschema=$(ldms_rdc_schema_name metrics=v10|grep -v ERR)
export wschema
export dschema
DAEMONS $(seq 3)
LDMSD 3

LDMSD 2
SLEEP 3
MESSAGE ldms_ls on host 2:
LDMS_LS 2 -vl
SLEEP 2
KILL_LDMSD 2

LDMSD 1
SLEEP 3
MESSAGE ldms_ls on host 1:
LDMS_LS 1 -vl
SLEEP 2
KILL_LDMSD 1 3

file_created $STOREDIR/node/$wschema
file_created $STOREDIR/node/$dschema
